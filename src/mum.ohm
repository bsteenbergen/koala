mum {
    Program = Stmt+
    
    Stmt = print (float | int | str | id)		  --print
    	| TypeName id "=" (float | int | str | id)     --vardecl
        
    TypeName = "int" | "float" | "str"
    
    id = letter alnum*

    str = "\"" char* "\""
    int = ("-")? digit+
    float = ("-")? digit+ "." digit+ (("E" | "e") ("+" | "-")? digit+)?

    print = "print" ~alnum
        
    char = ~"\"" ~"\n" any 
    space += "@" (~"\n" any)* --comment 
}